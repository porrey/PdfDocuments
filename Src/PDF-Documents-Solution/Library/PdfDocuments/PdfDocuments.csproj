<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>net8.0</TargetFrameworks>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	  <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	  <PackageLicenseUrl></PackageLicenseUrl>
	  <Authors>Daniel M. Porrey</Authors>
	  <Company>Daniel M. Porrey</Company>
	  <Product></Product>
	  <Copyright>Copyright © Daniel Porrey 2021-2024. All Rights Reserved.</Copyright>
	  <Description>High level C# library for the easy creation of PDF documents using PdfSharp.</Description>
	  <Version>8.0.0</Version>
	  <FileVersion>8.0.0</FileVersion>
	  <AssemblyVersion>8.0.0</AssemblyVersion>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <PackageProjectUrl>https://github.com/porrey/PdfDocuments</PackageProjectUrl>
	  <RepositoryUrl>https://github.com/porrey/PdfDocuments</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>
	  <PackageIcon>PdfDcouments.png</PackageIcon>
	  <NeutralLanguage>en-US</NeutralLanguage>
	  <PackageIconUrl></PackageIconUrl>
	  <PublishRepositoryUrl>true</PublishRepositoryUrl>
	  <EmbedUntrackedSources>true</EmbedUntrackedSources>
	  <PublishRepositoryUrl>true</PublishRepositoryUrl>
	  <EmbedUntrackedSources>true</EmbedUntrackedSources>
	  <IncludeSymbols>true</IncludeSymbols>
	  <SymbolPackageFormat>snupkg</SymbolPackageFormat>
	  <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
	  <PackageReadmeFile></PackageReadmeFile>
	  <PackageTags>pdf;document;barcode</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.0" />
    <PackageReference Include="PDFsharpNetStandard2" Version="1.51.4845" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Images\PdfDcouments.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>

  <ItemGroup>
    <None Remove="Config\PdfDocument.QuoteDocument.config" />
    <None Remove="Images\approved.png" />
    <None Remove="Images\expired.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Config\PdfDocument.QuoteDocument.config">
